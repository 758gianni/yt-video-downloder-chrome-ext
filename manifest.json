{
    "name": "YT Vid Downloader",
    "description": "A chrome extension that adds a simple download button to YouTube.",
    "author": "Gianni Belizaire",
    "version": "1.0",
    "manifest_version": 3,
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "content-script.js"
            ],
            "run_at": "document_end"
        }
    ]
}